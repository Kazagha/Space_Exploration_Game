<div class="card">
    <!-- YOUR CUSTOM CODE STARTS HERE -->
			<div class="card_panel">
      <div class="card_art bg_">
      ARTWORK
      </div>
      <div class="card_title sg2 bg_a">
      {{card_name}}
      </div>
		<div class="abilities_list sg">
			<div class="ability validate" data-validate="{{a1_name}}">
				<div class="ability_desc bg_ ">
				<div class="ability_title sg1">== {{a1_name}}==</div>
				<div class="ability_text bg_a">{{a1_text}}</div>
				</div>
				<div class="ability_tag validate bg_ " data-validate={{a1_ico1}}>
				  <img class="tag_icon bg_" src="/image/{{a1_ico1}}.png">  
				  <div class="tag_num bg_">{{a1_num1}}</div>          
				</div>
				<div class="ability_tag validate bg_" data-validate={{a1_ico2}}>
				  <img class="tag_icon bg_" src="/image/{{a1_ico2}}.png">
				  <div class="tag_num bg_">{{a1_num2}}</div>  
				</div>
				<div class="ability_tag validate bg_ " data-validate={{a1_ico3}}>
				  <img class="tag_icon bg_" src="/image/{{a1_ico3}}.png">
				  <div class="tag_num bg_">{{a1_num3}}</div>
				</div>

			</div>
			<div>
			
			</div>
		</div>
		</div>
		<script>
		// Get all children that require validation 
		var children = document.getElementsByClassName("validate");		
		removeBlank(children);
		
		function removeBlank(children) {
			for (var i = 0; i < children.length; i++) {
			  var child = children[i];  
			  // Fetch the value to be checked for validotion. 
			  // Either the name of the ability or the tag icon.
			  var validate = child.dataset.validate;
			  // Check if the ability/tag is valid 
			  if (validate == "") {
				console.log("Nothing found, hiding this element")
				// Add the 'hide' class to this element to remove it from the card
				child.classList.add("hide");
				
				
				} else {
				console.log("Found: " + validate); 
				}
			// Remove the 'validate' class tag so this element won't be checked again 
			child.classList.remove("validate");
			}
		}
		</script>
		
    <!-- YOUR CUSTOM CODE ENDS HERE -->

</div>